        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name=".Activities.WelcomeScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Activities.Home" android:label="@string/app_name" />

        <activity
            android:name=".Activities.ExperimentTable" android:label="@string/app_name" />

        <activity
            android:name=".Activities.PlotGraph" android:label="@string/app_name" />

    </application>

</manifest>
